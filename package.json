{
  "name": "typex",
  "description": "Unix CLI that provides salient information about installed commands, programs, and filesystem items, combining elements from standard Unix utilities type, which, and file.",
  "private": false,
  "version": "0.4.2",
  "os": [
    "darwin",
    "linux"
  ],
  "preferGlobal": true,
  "bin": {
    "typex": "bin/typex"
  },
  "homepage": "https://github.com/mklement0/typex",
  "repository": {
    "type": "git",
    "url": "https://github.com/mklement0/typex"
  },
  "bugs": {
    "url": "https://github.com/mklement0/typex/issues"
  },
  "scripts": {
    "test": "make test",
    "install": "printf '%s\\n\\n' \"IMPORTANT: To install sourcing for supported shells, run: typex -i\""
  },
  "keywords": [
    "type",
    "which",
    "shell",
    "version",
    "location",
    "path",
    "file",
    "command",
    "definition",
    "cli"
  ],
  "author": "Michael Klement <mklement0@gmail.com> (http://same2u.net)",
  "license": "MIT",
  "devDependencies": {
    "doctoc": "^0.13.0",
    "json": "^9.0.3",
    "marked-man": "^0.1.5",
    "replace": "^0.3.0",
    "semver": "^4.2.0",
    "shall": "^0.2.1",
    "urchin": "^0.0.5"
  },
  "net_same2u": {
    "make_pkg": {
      "tocOn": true,
      "tocTitle": "**Contents**",
      "manOn": true
    }
  },
  "man": "./man/typex.1"
}
