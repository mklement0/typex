{
  "name": "typex",
  "description": "An extended version of the `type` Unix utility.",
  "private": false,
  "version": "0.1.2",
  "os": [
    "darwin",
    "linux"
  ],
  "preferGlobal": true,
  "bin": {
    "typex": "bin/typex"
  },
  "homepage": "https://github.com/mklement0/typex",
  "repository": {
    "type": "git",
    "url": "https://github.com/mklement0/typex"
  },
  "bugs": {
    "url": "https://github.com/mklement0/typex/issues"
  },
  "scripts": {
    "test": "make test",
    "install": "printf '%s\\n\\n' \"IMPORTANT: To install sourcing for supported shells, run typex -i\""
  },
  "keywords": [
    "type",
    "which",
    "shell",
    "version",
    "location",
    "path",
    "file",
    "command",
    "definition"
  ],
  "author": "Michael Klement <mklement0@gmail.com> (http://same2u.net)",
  "license": "MIT",
  "devDependencies": {
    "json": "^9.0.3",
    "replace": "^0.3.0",
    "semver": "^4.2.0",
    "shall": "^0.1.3",
    "urchin": "^0.0.5"
  }
}
